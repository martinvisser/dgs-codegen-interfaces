type Query {
    profile: Profile
}

type Profile {
    id: ID!
    agreements: [Agreement!]!
}

interface Agreement {
    id: ID!
}

interface Account implements Agreement {
    id: ID!
    name: String!
    balance: Float!
}

type PaymentAccount implements Agreement & Account {
    id: ID!
    name: String!
    balance: Float!
    currency: String!
}

type SavingsAccount implements Agreement & Account {
    id: ID!
    name: String!
    balance: Float!
    interestRate: Float!
}
